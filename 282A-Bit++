#include<iostream>
#include<string>

using namespace std;
int main()
{   int t,x=0;
    string s;
    cin>>t;
    while(t>0)
    {
        cin>>s;
        if(s=="X++" || s=="++X")
        {
            x=x+1;
        }
        if(s=="X--" || s=="--X")
        {
            x=x-1;
        }
        t--;

    }
    cout<<x<<endl;


    return 0;
}
