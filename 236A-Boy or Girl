#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    int i,l,c=0;
    cin>>s;
    l=s.length();
    sort(s.begin(),s.end());
    for(i=1;i<l;i++)
    {
            if(s[i]!=s[i-1])
            {
                c++;
            }
    }
    if((c+1)%2==0)
    {
        cout<<"CHAT WITH HER!"<<endl;
    }
    else
    {
        cout<<"IGNORE HIM!"<<endl;
    }

    return 0;
}
